<div class="container panel panel-body">
    <div class="col-md-8 col-md-offset-2">
    <h1>Register</h1>
        <form class="form-horizontal">   
            <div class="form-group">
                <input type="text" ng-model="user.username" class="form-control" placeholder="username">
            </div>
            <div class="form-group">
                <input type="password" ng-model="user.password" class="form-control" placeholder="password">
            </div>
            <button ng-click="register()" class="btn btn-primary btn-block">Register</button>
        </form>
    </div>
</div>